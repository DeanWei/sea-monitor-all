    {
      "name": "sea.monitor.filter.exclude",
      "type": "java.lang.String",
      "sourceType": "com.github.seaframework.monitor.boot.autoconfigure.SeaMonitorProperties",
      "defaultValue": "",
      "description": "sea monitor filter exclude url."
    },
    {
      "name": "sea.monitor.filter.order",
      "type": "java.lang.Integer",
      "sourceType": "com.github.seaframework.monitor.boot.autoconfigure.SeaMonitorProperties",
      "defaultValue": 1,
      "description": "sea monitor filter order, default 1."
