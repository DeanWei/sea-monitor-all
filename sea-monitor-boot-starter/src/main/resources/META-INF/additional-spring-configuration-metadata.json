      "description": "区域名称，推荐使用拼音或英文"
    },
    {
      "name": "sea.monitor.enabled",
      "type": "java.lang.Boolean",
      "sourceType": "com.github.seaframework.monitor.boot.autoconfigure.SeaMonitorProperties",
      "defaultValue": false,
      "description": "是否启用SeaMonitor,默认false."
    },
    {
      "name": "sea.monitor.endpoint",
      "type": "java.lang.String",
      "sourceType": "com.github.seaframework.monitor.boot.autoconfigure.SeaMonitorProperties",
      "defaultValue": "",
      "description": "应用名称"
    },
    {
      "name": "sea.monitor.mode",
      "type": "java.lang.String",
      "sourceType": "com.github.seaframework.monitor.boot.autoconfigure.SeaMonitorProperties",
      "defaultValue": "0",
      "description": "log mode. 0 report to remote server (by default); 1 log to local log file"
